{
  "name": "awesome-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:docs",
    "start": "ng serve",
    "build:docs": "ts-node scripts/build.ts",
    "watch": "chokidar './src/assets/content/**/*.md' --initial -c 'npm run build:docs || true'",
    "prebuild": "npm run build:docs",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prebuild:ssr": "ng run awesome-angular:server",
    "build:ssr": "tsc -p tsconfig.ssr.json",
    "serve:ssr": "node dist/server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ts-node ./scripts/pre-commit.ts"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "core-js": "^2.6.5",
    "github-slugger": "^1.2.1",
    "highlight.js": "^9.15.6",
    "marked": "^0.6.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@awesome-fe/prerender": "0.0.5",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@types/chokidar": "^1.7.5",
    "@types/glob": "^7.1.1",
    "@types/highlight.js": "^9.12.3",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.121",
    "@types/marked": "^0.6.1",
    "@types/node": "^11.10.4",
    "chai": "^4.2.0",
    "chokidar": "^2.1.2",
    "chokidar-cli": "^1.2.2",
    "codelyzer": "^5.0.1",
    "cross-spawn": "^6.0.5",
    "glob": "^7.1.3",
    "http-server-spa": "^1.3.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lodash": "^4.17.11",
    "mocha": "^6.0.2",
    "protractor": "~5.4.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "~5.13.1",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.2.3",
    "zone.js": "~0.9.1"
